h3 { font-size: 2em; }

// Progress Bars
progress, progress[role] {
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
  border: none;
  background-size: auto;
  height: 100px;
  width: 100%;
}

// The unordered list
.skill-list {
  list-style: none;
